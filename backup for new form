        else if(bmi >= 18.6 && bmi <= 24.9)
        {
          $(".bmiresults").append("<h2>"+bmi+"</h2><p>which is Normal.</p>")
          bmiscore=0;
        }
        else if(bmi >= 25.0 && bmi <= 29.9)
        {
          $(".bmiresults").append("<h2>"+bmi+"</h2><p>which is overweight.</p>")
          bmiscore=3;
        }
        else if(bmi >= 30.0)
        {
          $(".bmiresults").append("<h2>"+bmi+"</h2><p>which is obese.</p>")
          bmiscore=5;
        }
      }
      else
      {
        $(".sleep-step4-next").hide();
      }
