        var getUrlParameter = function getUrlParameter(sParam) {
          var sPageURL = window.location.search.substring(1),
              sURLVariables = sPageURL.split('&'),
              sParameterName,
              i;

          for (i = 0; i < sURLVariables.length; i++) {
            sParameterName = sURLVariables[i].split('=');

            if (sParameterName[0] === sParam) {
              return typeof sParameterName[1] === undefined ? true : decodeURIComponent(sParameterName[1]);
            }
          }
          return false;
        };

        var urlRef;
        var tech = getUrlParameter('ref');
        var partnerName = getUrlParameter('utm_term');

        if(tech) {
          urlRef = tech;
        } else {
          urlRef = '';
        }

        // Create the new request 
        var xhr = new XMLHttpRequest(); 
        var url = 'https://api.hsforms.com/submissions/v3/integration/submit/3445757/{{ module.form_id }}'

        // Example request JSON:
        var data = {
          "fields": [
            {
              "name": "email",
              "value": email
            },
            {
              "name": "firstname",
              "value": name1
            },
            {
              "name": "lastname",
              "value": surename
            },
            {
              "name": "gender_one",
              "value": gender
            },
            {
              "name": "what_is_your_height_in_cms",
              "value": height
            },
            {
              "name": "what_is_your_weight_in_kgs",
              "value": weight
            },
            {
              "name": "what_is_your_neck_circumference",
              "value": neck
            },
            {
              "name": "date_of_birth",
              "value": daydate
            },
            {
              "name": "do_you_snore",
              "value": snore
            },
            {
              "name": "phone",
              "value": phno
            },
            {
              "name": "score",
              "value": score
            },
            {
              "name": "result",
              "value": rsult
            },
            {
              "name":"sleep_assessment_status",
              "value":"True"
            },
            {
              "name": "leadsource",
              "value": urlRef
            },
            {
              "name": "partner_name",
              "value": partnerName
            },
            {
              "name": "page_link",
              "value": currentURL
            }

              ],
              "context": {
              "hutk": document.cookie.replace(/(?:(?:^|.*;\s*)hubspotutk\s*\=\s*([^;]*).*$)|^.*$/, "$1"),
              "pageUri": currentURL,
              "pageName": currentPageTitle
            },
