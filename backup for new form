    $( ".sleep-step3-submit" ).click(function() {

      var phemail = $(".phnumber").val();
      var emailIp = $(".email-val").val();
      var filter_mob = /^\d*(?:\.\d{1,2})?$/;
      var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;

      if(filter_mob.test(phemail)) {
        if((phemail.length>=7) && (phemail.length<=15)) {
          phnumber = 1;
          $('span.alert-mess-req-num').hide();
          $('span.alert-mess-valid-ph').hide();
        } else {
          phnumber = 0;
          $('span.alert-mess-valid-ph').show();
        }
      }

      var country_from_URL = $(location).attr('hostname');
      var country_from_URL_array = country_from_URL.split('.');
      var parse_Country = country_from_URL_array[country_from_URL_array.length-1],Country_val;
      console.log(parse_Country);
      if(parse_Country == 'tw'){
        if(filter_mob.test(phemail)) {
          if((phemail.length<=15)) {
            phnumber = 1;
            $('span.alert-mess-req-num').hide();
            $('span.alert-mess-valid-ph').hide();
          } else {
            phnumber = 0;
            $('span.alert-mess-valid-ph').show();
          }

          if(phemail == '') {
            phnumber = 0;
            $('span.alert-mess-valid-ph').show();
          } else {
            phnumber = 1;
          }
        }
      }

      if(!filter.test(emailIp)){
        console.log(emailIp + ' - invalid email format');
        emailtrue = 0;
        $('.sleep-steps-inner1 p span.req-email').hide();
        $('.sleep-steps-inner1 p span.val-email').show();
        return false;
      } else {
        $('.sleep-steps-inner1 p span.val-email').hide();
      }

      if(emailIp == '') {
        emailtrue = 0;
        $('.sleep-steps-inner1 p span.val-email').hide();
        $('.sleep-steps-inner1 p span.req-email').show();
        //       $(".sleep-step3").show();
        //       $(".sleep-step4").hide();
      } else {
        console.log(emailIp + " set to 1");
        emailtrue = 1;
      }

      if(phemail == '') {
        phnumber = 0;
        $('span.alert-mess-req-num').show();
        //         $(".sleep-step3").show();
        //         $(".sleep-step4").hide();
        $('span.alert-mess-valid-ph').hide();
      } 

      /*if(phemail.charAt(0) == "0") {
      console.log('chatAt(0) == zero');
      phnumber = 0;
      $('span.alert-mess-req-num').hide();
      $('span.alert-mess-valid-num').show();
    }*/

      console.log('phnumber == ' + phnumber);
      console.log('emailtrue == ' + emailtrue);

      if((phnumber == 1) && (emailtrue == 1)) {
        emailtrue = 1;
        phnumber = 1;
        $('.sleep-steps-inner1 p span.val-email').hide();
        $('.sleep-steps-inner1 p span.req-email').hide();
        $('span.alert-mess-req-num').hide();
        //         $(".sleep-step3").hide();
        //         $(".sleep-step4").show( "slide", {direction: "right" }, 1000 );
      }

