        $('span.alert-mess-req-num').hide();
        $(".sleep-step3").hide();
        $(".sleep-step4").show( "slide", {direction: "right" }, 1000 );
      }
    });

    $( ".sleep-step4-next" ).click(function() {
      $(".sleep-step4").hide();
      $(".sleep-step5").show( "slide", {direction: "right" }, 1000 );
    });

    $( ".sleep-step5-next" ).click(function() {
      $(".sleep-step5").hide();
      $(".sleep-step6").show( "slide", {direction: "right" }, 1000 );
    });

    $( ".sleep-step6-next" ).click(function() {
      $(".sleep-step6").hide();
      $(".sleep-step8").show( "slide", {direction: "right" }, 1000 );
    });

    $( ".sleep-step7-input button" ).click(function() {
      $(this).addClass("active");
      $(".sleep-step7").hide();
      $(".sleep-step8").show( "slide", {direction: "right" }, 1000 );
    });


    $( ".sleep-step8-input input" ).click(function() {
      $(this).addClass("active").siblings('input').removeClass("active");
      console.log($(this).val());
      if($(this).val()=="Male")
      {
        genderscore = 2;
      }
      $(".sleep-step8").hide();
      $(".sleep-step9").show( "slide", {direction: "right" }, 1000 );
    });

    $( ".sleep-step9-input input" ).click(function() {
      $(this).addClass("active").siblings('input').removeClass("active");
      console.log($(this).val());
      if($(this).val()=="Yes")
      {
        snorescore = 2;
      }
      $('.sleep-step9-next').show();
    });

    $('.sleep-step9-next').click(function() {

      score = bmiscore + agescore + neckscore + genderscore + snorescore;
      console.log(score);

      $(".sleep-step9").hide();

      $(".totalscore").append(score);
      if(score >= 8){

        $(".totalscore1").append("Positive");
        $(".totalscoreimage").append("<img src='https://cdn2.hubspot.net/hubfs/3445757/FINAL-HR.2.gif'>");
        formv3();
        setInterval(function () {
          window.location.href="{{ module.high_score_url }}";
        },3000);

      } else { 

        $(".totalscore1").append("Negative");
        $(".totalscore1").append("<img src='https://cdn2.hubspot.net/hubfs/3445757/FINAL-LR.2.gif'>");
        formv3(); 
        setInterval(function () { 
          window.location.href="{{ module.low_score_url }}";
        },3000);

      }
      $(".step2-form .hs-button").click();
    });


  });

</script> 


