    <script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/v2.js"></script>
    <script>
      $(document).ready(function(){

        var country_from_URL = $(location).attr('hostname');
        var country_from_URL_array = country_from_URL.split('.');
        var parse_Country = country_from_URL_array[country_from_URL_array.length-1],Country_val;
        console.log(parse_Country);

        var dateISO;
        var d = new Date();
        d.setUTCHours(0,0,0,0);
        dateISO = d.toISOString();
        //console.log(Date.parse(dateISO));

        if(parse_Country == 'vn'){
          Country_val = 'Vietnam';
          $("a.privacy_url").attr("href", "https://www.resmed.vn/en-vn/index/privacy");
          $("a.tnc_url").attr("href", "https://www.resmed.vn/en-vn/index/terms-of-use");
        }
        else if(parse_Country == 'la'){
          Country_val = 'Laos';
          $("a.privacy_url").attr("href", "https://www.resmed.la/en-la/index/privacy");
          $("a.tnc_url").attr("href", "https://www.resmed.la/en-la/index/terms-of-use");
        }
        else if(parse_Country == 'id'){
          Country_val = 'Indonesia';
          $("a.privacy_url").attr("href", "https://www.resmed.co.id/en-id/index/privacy");
          $("a.tnc_url").attr("href", "https://www.resmed.co.id/en-id/index/terms-of-use");
        }
        else if(parse_Country == 'th'){
          Country_val = 'Thailand';
          $("a.privacy_url").attr("href", "https://www.resmed.co.th/en-th/index/privacy");
          $("a.tnc_url").attr("href", "https://www.resmed.co.th/en-th/index/terms-of-use");
        }
